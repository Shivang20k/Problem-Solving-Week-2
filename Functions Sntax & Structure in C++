1.]WAP addition->

#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

int sum(int , int);

int main(){
    int a, b;
    cin>>a>>b;
    int c =sum(a,b);
    cout<< c;
    return 0;
}

int sum(int x, int y)
{return x+y;}



2.] Maximum of 3 elements
#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

int maximum(int , int, int);

int main(){
    int a, b, c;
    cin>>a>>b>>c;
    int m =maximum(a,b, c);
    cout<< m;
    return 0;
}

int maximum(int x, int y, int z)
{
    if(x>y && x>z)
        return x;
    else if(y>x && y>z)
        return y;
    else
        return z;
}


3.]Print Count

#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

void count(int);

int main(){
    int a;
    cin>>a;
    count(a);
    return 0;
}

void count(int x)
{
    int i =1;
    
    while(i<=x){
        cout<<i<<", ";
        i+=1;
    }
   
}



4]Sum  of Even Number upto N

#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

int sumOfEven(int);

int main(){
    int a;
    cin>>a;
    cout<<sumOfEven(a);
    return 0;
}

int sumOfEven(int x)
{
    int i =2;
    int sum =0;
    while(i<=x){
        sum+=i;
        i+=2;
    }
   return sum;
}

